    "laravel-mix": "^2.0",
    "lodash": "^4.17.20",
    "popper.js": "^1.12",
    "tailwindcss-plugins": "^0.3.0",
    "vue": "^2.6.11"
  },
  "dependencies": {
    "alpinejs": "^2.3.5",
    "botman-tinker": "^0.0.1",
    "tailwindcss": "^1.6.2",
    "turbolinks": "^5.2.0",
    "v-calendar": "^1.0.8",
    "v-calendar-scheduler": "^0.7.4",
    "vue-trix": "^1.1.9"
  }
